import csv

F = open("task14.csv", encoding="utf-8")
csvv = [i for i in csv.reader(F, delimiter=",")]
while True:
    a = input()
    if a == "СТОП":
        break
    else:
        for j in range(len(csvv)):
            if csvv[j][0] == a:
                print(csvv[j][0], csvv[j][1][0], csvv[j][3], csvv[j][4], csvv[j][2])
